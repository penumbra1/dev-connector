{
    "presets": [
        ["@babel/env", {
          "useBuiltIns": "usage",
          "targets": "> 0.25%, not dead",
          "modules": false
          }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        ["import", {
            "libraryName": "antd",
            "style": true
        }]
    ]
}